{
  "Kestrel": {
    "Endpoints": {
      "Public": {
        "Url": "http://*:5000"
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Console": {
    "TimestampFormat": "'['dd'.'MM'.'yy' 'HH':'mm':'ss'.'fff']'",
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "PolicyServiceSettings": {
    "TimeoutInSeconds": 60,
    "RetryCount": 5,
    "StatusCodesWorthRetrying": [ "500", "502", "503", "504" ]
  },
  "ApplicationSettings": {
    "CorsAllowedOrigins": [ "*" ]
  },
  "AllowedHosts": "*"
}
